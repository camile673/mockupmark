<form action="/mockup" method="post" enctype="multipart/form-data">
  <label for="file-upload" class="custom-file-upload">
    <i class="fa fa-cloud-upload"></i>
  </label>
  <input id="file-upload" type="file" name="artwork" accept=".png, .jpg, .jpeg" required/>
  <button type="submit" onclick="validate()">Upload</button>
</form>
