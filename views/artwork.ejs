<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <div class="masonry-wrapper">
      <div class="masonry">
        <% images.forEach(image => { %>
          <div class="masonry-item">
            <img class="masonry-content" src="<%= image %>" />
          </div>
        <% }) %>
      </div>
    </div>

  </body>
</html>
